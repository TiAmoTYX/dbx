.the_header{
  margin: 20rpx 0 0 0;
}
.the_header text{
  display: block;
  width: 100%;
  font-size: 28rpx;
  text-align: center;
}
.the_header image{
  display: block;
  width: 102rpx;
  height: 102rpx;
  margin: 20rpx auto 0 auto;
}
.button1{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 700rpx;
}
.button1 image{
  display: block;
  margin: 0 auto;
  width: 270rpx;
  height: 270rpx;
}
.button1 image:active{
  width: 260rpx;
  height: 260rpx;
}